generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Clientes {
  id       Int      @id @default(autoincrement())
  nome     String
  email    String   @unique
  senha    String   @unique
}

model Produtos {
  id           Int     @id @default(autoincrement())
  nomeproduto  String
  preco        Float
  precovenda   Float
  validade     DateTime
  quantidade   Int
  dataregistro    DateTime @default(now())
  attregistro    DateTime @updatedAt
}
